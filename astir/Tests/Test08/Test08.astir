// Test08 - a context-free grammar LL(1) parser test

// The following is actually left-recursive
/*
LL(2) parser TreeParser with ambiguity_resolved_by_precedence {
	root production Node =
		'(' Node ')'
		| '(' Node Node ')'
		| 'L' // as in "leaf"
		;
}
*/

LL(2) parser TreeParser with ambiguity_resolved_by_precedence {
	root production Node =
		'(' ')' // empty
		| '(' Node Node ')'
		| 'L' // as in "leaf"
		;
}